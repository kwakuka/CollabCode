<!doctype html>
<html lang="en">
  <head>
    <title>RealTimeWeekly</title>
  </head>
  <body>
  <script src='http://static.opentok.com/webrtc/v2.2/js/opentok.min.js'></script>
  <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
  
  <script type="text/javascript">
    // REMEMBER TO CREATE AN ACCOUNT WITH TOKBOX AND THEN POPULATE YOUR API_KEY, SESSION_ID, AND TOKEN IN THIS CODE
    // Initialize session, set up event listeners, and connect
    var session = OT.initSession("45305282", "2_MX40NTMwNTI4Mn5-MTQzODk2MTAyNzMzM35CcDJaVmoxeUhZUForM2o4UFdSR2NKYU5-fg");
    // Connect to our session and publish our feed
    session.connect("2_MX40NTMwNTI4Mn5-MTQzODk2MTAyNzMzM35CcDJaVmoxeUhZUForM2o4UFdSR2NKYU5-fg", function(error) {
      publisher = OT.initPublisher( "publisher");
      session.publish(publisher);
    });
    // When a client connects, subscribe to its feed
    session.on( {
      streamCreated: function(event) {
        session.subscribe(event.stream, "subscriber");
    }});
  </script>
  <div class="video">
    <div id="publisher"></div>
    <div id="subscriber"></div>
  </div>
</body>
</html>